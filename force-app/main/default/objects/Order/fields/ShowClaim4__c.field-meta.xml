<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ShowClaim4__c</fullName>
    <formula>OR(
  Claim_3_Amount__c &gt; 0,
  NOT(ISBLANK(Claim_3_Invoice_Number__c)),
  NOT(ISBLANK(Claim_3_Date__c)),
  NOT(ISBLANK(Claim_3_Invoice_Date_Paid__c)),
  Claim_3_Updated_System__c &gt; 0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>ShowClaim4</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
