<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ShowClaim3__c</fullName>
    <formula>OR(
  Claim_2_Amount__c &gt; 0,
  NOT(ISBLANK(Claim_2_Invoice_Number__c)),
  NOT(ISBLANK(Claim_2_Date__c)),
  NOT(ISBLANK(Claim_2_Invoice_Date_Paid__c))
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>ShowClaim3</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
