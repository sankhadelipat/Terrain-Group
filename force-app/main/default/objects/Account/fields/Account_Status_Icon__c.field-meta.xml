<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Status_Icon__c</fullName>
    <description>Created by BDD</description>
    <externalId>false</externalId>
    <formula>CASE(TEXT( Account_Status__c ), 
&quot;Active&quot;, IMAGE(&quot;/resource/icon_active_account&quot;, &quot;Active&quot;, 32, 33),
&quot;Inactive&quot;, IMAGE(&quot;/resource/icon_inactive_account&quot;, &quot;Inactive&quot;, 32, 33),
&quot;Blacklisted&quot;, IMAGE(&quot;/resource/icon_blacklisted_account&quot;, &quot;Blacklisted&quot;, 32, 33),
IMAGE(&quot;/resource/icon_inactive_account&quot;, &quot;Inactive&quot;, 32, 33))</formula>
    <label>Account Status</label>
    <required>false</required>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
